{
  "name": "util-state",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/shared/util-state/src",
  "prefix": "lib",
  "projectType": "library",
  "tags": [
    "type:util",
    "scope:shared"
  ],
  "targets": {
    "build": {
      "executor": "@nx/angular:ng-packagr-lite",
      "outputs": ["{workspaceRoot}/dist/{projectRoot}"],
      "options": {
        "project": "libs/shared/util-state/ng-package.json",
        "tsConfig": "libs/shared/util-state/tsconfig.lib.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/shared/util-state/tsconfig.lib.prod.json"
        },
        "development": {}
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nx/vitest:test",
      "outputs": [
        "{options.reportsDirectory}"
      ],
      "options": {
        "reportsDirectory": "../../../coverage/libs/shared/util-state"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    }
  }
}
